{
	"$schema": "dpe/json/schema",
  "$libraries": {
    "rawJsonText": "rawJsonText-library.json"
  },
	"description": "Configuration file defining the metadata of a data pack for Minecraft.",
	"$body": {
		"$type": "object",
		"properties": {
			"pack": {
				"description": "Pack",
				"$type": "object",
				"properties": {
					"pack_format": { "$type": "integer" },
					"description": { "$defRef": "rawJsonText:rawJsonText" }
				}
			}
		}
	}
}